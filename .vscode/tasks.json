{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe générer le fichier actif",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/Fe:",
        "${fileDirname}\\Debug\\${fileBasenameNoExtension}.exe",
        "${file}"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Tâche générée par le débogueur."
    }
  ],
  "version": "2.0.0"
}